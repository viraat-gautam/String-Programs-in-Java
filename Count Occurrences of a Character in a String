class String1 {
    public static void main(String[] args) {
        String str = "adsffhgkljqtwyuioASHGDJJKLKZXNBCM";
        int count = 0;
        String str1 = str.toLowerCase();
        for (int i = 0; i < str1.length(); i++) {
            if (str1.charAt(i) == str1.charAt(0)) {
                count++;

            }
        }
        System.out.println(count);
    }
}
